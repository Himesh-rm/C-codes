#include<iostream>
using namespace std;

int main(){
    // I-Part
    for(int i=0;i<6;i++){
        for(int j=0;j<3;j++){
            cout << "*";
        }
        for(int j=0;j<2;j++){
            cout << " ";
        }
        
        // L-Part
        for(int j=0;j<3;j++){
            cout << "*";
        }
        for(int j=0;j<5;j++){
            if(i>=0 && i<=3) cout << " ";
            if((i==4 || i==5) && j<=4){
                cout << "*";
            }
        }
        for(int j=0;j<2;j++){
            cout << " ";
        }
        
        // O-Part
        for(int j=0;j<8;j++){
            cout << "*";
        }
            cout << endl;
    }
    return 0;
}
